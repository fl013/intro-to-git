<%- include("partials/header") %>

<div class="container">
	<header class="jumbotron">
	<div class="container">
		<h1>This is our campgrounds page </h1>
	<p>
		View our hand-picked campgroudns
		</p>
		<p>
			<a class="btn btn-primary btn-lg" href="campgrounds/new"> Add a new campground</a>
		</p>
		</div>
	
</header>
</div>



<div class="row text-center" style="display:flex; flex-wrap:wrap;">
	
		<% campgrounds.forEach(function(camp){ %>
	<div class="col-md-3 col-sm-6">
		
		<div class = "thumbnail">
				<img src="<%= camp.image %> ">
			<div class="caption">
				<h4>
					<%= camp.name; %>
				</h4>
			</div>
		</div>
	</div>
	<% }) %>
		
	
	


<a href="/">View on Landing</a>

<%- include("partials/footer") %>